  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OrderConstraint">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="priorityLevel" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
