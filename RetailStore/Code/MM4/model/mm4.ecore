    <eStructuralFeatures xsi:type="ecore:EReference" name="orderconstraint" upperBound="-1"
        eType="#//OrderConstraint" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DeliveryConstraint">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="constraintID" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
